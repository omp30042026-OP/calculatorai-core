    "decision:store:sqlite": "tsx examples/run-decision-store-sqlite.ts > /dev/null",
    "decision:snapshots:sqlite": "tsx examples/run-decision-snapshots-sqlite.ts > /dev/null",
    "decision:durability:sqlite": "tsx examples/run-decision-durability-sqlite.ts > /dev/null",
    "decision:retention:sqlite": "tsx examples/run-decision-retention-sqlite.ts > /dev/null",
    "decision:history:sqlite": "tsx examples/run-decision-history-sqlite.ts > /dev/null",
