    "decision:diff:sqlite": "tsx examples/run-decision-diff-sqlite.ts > /dev/null",
    "decision:audit:sqlite": "tsx examples/run-decision-audit-sqlite.ts > /dev/null",
    "decision:timeline:sqlite": "tsx examples/run-decision-timeline-sqlite.ts > /dev/null",
    "decision:fork-graph:sqlite": "tsx examples/run-decision-fork-graph-sqlite.ts > /dev/null",
    "decision:lineage:sqlite": "tsx examples/run-decision-lineage-sqlite.ts > /dev/null",
    "decision:observability:sqlite": "tsx examples/run-decision-observability-sqlite.ts > /dev/null",
